{
  "name": "dog-things-vue",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "prod": "incremental dist/index.html",
    "push-ghpages": "cd dist && git init && git remote add origin git@github.com:bitovi/dog-things-vue.git && git checkout -b gh-pages && git add -A && git commit -m site && git push -f origin gh-pages && cd ..",
    "deploy": "npm run build && npm run push-ghpages",
    "heroku-postbuild": "npm run build"
  },
  "dependencies": {
    "@bitovi/incremental": "^1.0.2",
    "can-ndjson-stream": "^1.0.0",
    "vue": "^2.5.21"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.3.0",
    "@vue/cli-plugin-eslint": "^3.3.0",
    "@vue/cli-service": "^3.3.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0",
    "vue-template-compiler": "^2.5.21"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
